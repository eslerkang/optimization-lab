   "source": [
    "import pandas as pd\n",
    "\n",
    "types = [1, 2, 3]\n",
    "\n",
    "names = [f\"Lot {i}\" for i in range(1, 16)]\n",
    "\n",
    "pairs = [(t, n, 0) for t in types for n in names]\n",
    "\n",
    "data = pd.DataFrame(pairs, columns=['type', 'name', 'release'])\n",
    "\n",
    "data.to_csv('lot_data.csv', index=False)\n"
   ]
